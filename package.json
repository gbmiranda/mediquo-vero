{
  "name": "mediquo-vero",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8081",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "build:amplify": "NEXT_TELEMETRY_DISABLED=1 next build",
    "build:export": "NEXT_TELEMETRY_DISABLED=1 next build && next export",
    "preview": "npx serve out",
    "start": "pnpm run preview",
    "lint": "next lint",
    "deploy:build": "pnpm run build",
    "deploy:preview": "pnpm run build && pnpm run preview",
    "aws:sync": "aws s3 sync ./out s3://$S3_BUCKET_NAME --delete --cache-control 'public,max-age=31536000,immutable' --exclude '*.html' && aws s3 sync ./out s3://$S3_BUCKET_NAME --delete --cache-control 'public,max-age=0,must-revalidate' --include '*.html'",
    "aws:invalidate": "aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_DISTRIBUTION_ID --paths '/*'",
    "deploy:aws": "pnpm run build && pnpm run aws:sync && pnpm run aws:invalidate"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-toast": "1.2.4",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "next": "15.2.4",
    "next-themes": "^0.4.6",
    "nodemailer": "latest",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.58.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "latest"
  },
  "devDependencies": {
    "@types/node": "^22.15.32",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
